{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@screens/*": ["src/screens/*"],      
      "@assets/*": ["src/assets/*"],            
      "@services/*": ["src/services/*"],
      "@models/*": ["src/models/*"],
      "@storage/*": ["src/storage/*"],     
      "@utils/*": ["src/utils/*"],       
    }
  }
}
